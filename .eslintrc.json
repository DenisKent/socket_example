{
  "extends": "airbnb-base",
  "rules": {
    "global-require": 0,
    "no-bitwise": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "quotes": ["error", "double"],
    "no-extra-boolean-cast": 0,
    "object-curly-newline": 0,
    "allowMultiplePropertiesPerLine": true,
    "no-param-reassign": 0,
    "import/order": 0,
    "max-len": 0,
    "no-else-return": 0,
    "class-methods-use-this": 0,
    "newline-after-import": 0,
    "consistent-return": 0,
    "no-tabs": 0,
    "indent": ["error", 2],
    "linebreak-style": 0,
    "newline-per-chained-call": 0,
    "arrow-body-style": 0,
    "operator-linebreak": 0,
    "no-return-assign": 0,
    "camelcase": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "no-console": 0,
    "no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }
    ],
    "prefer-destructuring": ["error", { "object": true, "array": false }]
  },
  "env": {
    "jest": true
  },
  "globals": {
    "config": false,
    "logger": false,
    "document": false,
    "io": false,
    "window": false
  }
}
